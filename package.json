  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci --reporters=default --reporters=jest-junit",
    "test:enterprise": "jest --coverage --watchAll=false --ci --reporters=default --reporters=jest-junit --reporters=jest-html-reporters --reporters=jest-sonar",
    "test:performance": "jest --testNamePattern='performance' --logHeapUsage --detectOpenHandles",
    "test:security": "jest --testNamePattern='security|owasp|sanitization'",
    "test:accessibility": "jest --testNamePattern='accessibility|a11y|wcag'",
    "test:leak-detection": "node --expose-gc ./scripts/test-memory-leaks.js",
    "test:visual": "jest --testNamePattern='visual|snapshot' --updateSnapshot",
    "test:e2e": "detox test",
    "test:hooks": "jest src/features/*/presentation/hooks",
    "test:hooks:watch": "jest --watch src/features/*/presentation/hooks",
    "test:hooks:coverage": "jest --coverage src/features/*/presentation/hooks",
    "test:hooks:performance": "jest --testNamePattern='performance' src/features/*/presentation/hooks",
    "test:hooks:security": "jest --testNamePattern='security' src/features/*/presentation/hooks",
    "test:hooks:accessibility": "jest --testNamePattern='accessibility|a11y' src/features/*/presentation/hooks",
    "test:avatar": "jest src/features/profile/data/services/__tests__/avatar.service.test.ts src/features/profile/presentation/hooks/__tests__/use-avatar.hook.test.ts",
    "test:avatar:watch": "jest --watch src/features/profile/data/services/__tests__/avatar.service.test.ts src/features/profile/presentation/hooks/__tests__/use-avatar.hook.test.ts",
    "test:profile": "jest src/features/profile",
    "test:profile:watch": "jest --watch src/features/profile",
    "test:profile:enterprise": "jest --coverage --reporters=jest-html-reporters src/features/profile",
    "test:mutation-testing": "stryker run",
    "test:contract": "jest --testNamePattern='contract|api' --verbose",
    "test:integration": "jest --testNamePattern='integration' --runInBand",
    "test:stress": "jest --testNamePattern='stress|load' --maxWorkers=1",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "clean": "npx react-native clean",
    "clean:cache": "npx react-native start --reset-cache",
    "clean:deps": "rm -rf node_modules && npm install",
    "clean:ios": "cd ios && rm -rf Pods Podfile.lock && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "build:ios": "cd ios && xcodebuild -workspace ReactNativeSkeleton.xcworkspace -scheme ReactNativeSkeleton -configuration Release -destination 'generic/platform=iOS' -archivePath DaphkoAISkeleton.xcarchive archive",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios:debug": "cd ios && xcodebuild -workspace ReactNativeSkeleton.xcworkspace -scheme ReactNativeSkeleton -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 15' build",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:analyze": "npx react-native-bundle-visualizer --format json --output bundle-analysis.json",
    "fastlane:ios:beta": "cd ios && fastlane beta",
    "fastlane:ios:release": "cd ios && fastlane release",
    "prepare": "husky",
    "postinstall": "husky",
    "validate": "npm run lint && npm run type-check && npm run test:ci",
    "validate:avatar": "npm run lint && npm run type-check && npm run test:avatar",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "feature:create": "bash scripts/create_feature.sh",
    "feature:list": "bash scripts/list_features.sh",
    "feature:delete": "bash scripts/delete_feature.sh",
    "feature:update": "bash scripts/update_feature.sh",
    "feature:crud": "node scripts/create-crud-feature.mjs",
    "feature:cli": "node scripts/feature-cli.mjs",
    "docs:generate": "npx ts-node scripts/generate-docs.ts",
    "docs:features": "npm run docs:generate",
    "dev:menu": "node scripts/dev-menu.mjs",
    "dev:validate": "node scripts/validate-project.mjs",
    "monitoring:setup": "node scripts/setup-monitoring.mjs",
    "analytics:setup": "node scripts/setup-analytics.mjs",
    "performance:analyze": "npx react-native-bundle-visualizer",
    "security:audit": "npm audit && npx audit-ci --config audit-ci.json",
    "deps:check": "npx depcheck --ignores=\"@types/*,eslint-*,prettier,jest,@testing-library/*\"",
    "deps:fix": "node scripts/dependency-management.mjs fix",
    "deps:report": "node scripts/dependency-management.mjs report",
    "deps:validate": "npm run deps:check && npm run security:audit",
    "docker:dev": "docker-compose up rn-dev",
    "docker:test": "docker-compose run --rm rn-test",
    "docker:android": "docker-compose run --rm android-builder",
    "docker:security": "docker-compose run --rm security-audit",
    "docker:docs": "docker-compose up docs",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "bundle:analyze": "node scripts/bundle-analyzer.js analyze",
    "bundle:size": "npm run bundle:analyze",
    "bundle:optimize": "NODE_ENV=production npm run bundle:analyze",
    "bundle:report": "open bundle-analysis/bundle-report.html || xdg-open bundle-analysis/bundle-report.html",
    "adr:create": "node scripts/adr-manager.js create",
    "adr:list": "node scripts/adr-manager.js list",
    "adr:status": "node scripts/adr-manager.js status",
    "adr:validate": "node scripts/adr-manager.js validate",
    "adr:update": "node scripts/adr-manager.js update-index",
    "test:coverage:upload": "codecov",
    "pods:install": "cd ios && pod install --repo-update && cd ..",
    "security:check": "npm audit --audit-level=moderate",
    "license:check": "npx license-checker --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;0BSD'",
    "a11y:audit": "npx @axe-core/cli --dir ./src --include-dir",
    "a11y:test": "jest --testNamePattern='accessibility'",
    "a11y:report": "npm run a11y:audit && npm run a11y:test",
    "eslint:modern": "npx @eslint/config-inspector",
    "react:compiler": "npx react-compiler-healthcheck",
    "bundle:budget": "npx bundlesize",
    "vitals:monitor": "npm run performance:analyze && open vitals-report.html",
    "feature-flags:configure": "node scripts/build/configure-feature-flags.mjs",
    "feature-flags:basic": "node scripts/build/configure-feature-flags.mjs --variant=basic",
    "feature-flags:enterprise": "node scripts/build/configure-feature-flags.mjs --variant=enterprise",
    "feature-flags:development": "node scripts/build/configure-feature-flags.mjs --variant=development",
    "feature-flags:validate": "node scripts/build/configure-feature-flags.mjs --validate",
    "feature-flags:list": "node scripts/build/configure-feature-flags.mjs --list",
    "build:basic": "node scripts/build/build-variants.mjs --variant=basic --platform=android",
    "build:basic:ios": "node scripts/build/build-variants.mjs --variant=basic --platform=ios",
    "build:enterprise": "node scripts/build/build-variants.mjs --variant=enterprise --platform=android",
    "build:enterprise:ios": "node scripts/build/build-variants.mjs --variant=enterprise --platform=ios",
    "build:development": "node scripts/build/build-variants.mjs --variant=development --platform=android",
    "build:development:ios": "node scripts/build/build-variants.mjs --variant=development --platform=ios",
    "build:all-variants": "node scripts/build/build-variants.mjs --all-variants --platform=android",
    "build:all-variants:ios": "node scripts/build/build-variants.mjs --all-variants --platform=ios",
    "build:custom": "node scripts/build/build-variants.mjs",
    "build:validate": "node scripts/build/build-variants.mjs --pre-build-check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
